{
  "name": "adapt-keepScrollPosition",
  "version": "4.0.0",
  "framework": ">=2",
  "homepage": "https://github.com/cgkineo/adapt-keepScrollPosition",
  "issues": "https://github.com/cgkineo/adapt-keepScrollPosition/issues/",
  "extension": "keepScrollPosition",
  "targetAttribute": "_keepScrollPosition",
  "displayName": "Keep Scroll Position",
  "description": "An extension that will remember and restore the learner's scroll position within a menu or page",
  "main": "/js/adapt-keepScrollPosition.js",
  "keywords": [
      "adapt-plugin",
      "adapt-extension"
  ],
  "license": "GPL-3.0",
  "private": true,
  "repository": {
      "type": "git",
      "url": "git://github.com/cgkineo/adapt-keepScrollPosition"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.5",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "conventional-changelog-eslint": "^3.0.9",
    "semantic-release": "^19.0.3"
  },
  "scripts": {
    "postversion": "cp package.json bower.json"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "eslint"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "bower.json"
          ],
          "message": "Chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
